    "devDependencies": {
        "@types/parcel-env": "^0.0.0",
        "@types/react": "^17.0.9",
        "@types/react-dom": "^17.0.6",
        "@types/styled-components": "^5.1.9",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "eslint": "^7.28.0",
        "eslint-plugin-react": "^7.24.0",
        "parcel-bundler": "^1.12.5",
        "pre-commit": "^1.2.2",
